{
  "name": "heher-portfolio",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev:sst": "NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" sst dev",
    "dev:vite": "NODE_EXTRA_CA_CERTS=\"$(mkcert -CAROOT)/rootCA.pem\" sst bind remix vite:dev",
    "dev:server": "PORT=8080 sst bind remix dev",
    "dev:caddy": "caddy run",
    "newdev:server": "PORT=8080 sst bind 'remix dev -c \"node server.mjs\"'",
    "start": "remix-serve ./build/index.js",
    "typecheck": "tsc",
    "sst:deploy": "sst deploy",
    "deploy:staging": "sst deploy --stage staging",
    "deploy:prod": "sst deploy --stage prod",
    "gen": "graphql-codegen"
  },
  "dependencies": {
    "@react-three/drei": "^9.90.2",
    "@react-three/fiber": "^8.15.12",
    "@react-three/postprocessing": "^2.15.11",
    "@react-three/xr": "^5.7.1",
    "@remix-run/node": "^2.7.2",
    "@remix-run/react": "^2.7.2",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "d3": "^7.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "framer-motion": "^10.16.16",
    "framer-motion-3d": "^10.16.16",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "graphql-tag": "^2.12.6",
    "isbot": "^5.1.1",
    "lodash-es": "^4.17.21",
    "postprocessing": "^6.34.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use-measure": "^2.1.1",
    "three": "^0.159.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.0",
    "@graphql-codegen/client-preset": "^4.1.0",
    "@graphql-codegen/typescript-graphql-request": "^6.0.1",
    "@remix-run/dev": "^2.7.2",
    "@remix-run/eslint-config": "^2.7.2",
    "@types/d3": "^7.4.3",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@types/three": "^0.159.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.6.0",
    "autoprefixer": "^10.4.17",
    "aws-cdk-lib": "2.110.1",
    "concurrently": "^8.2.2",
    "constructs": "10.3.0",
    "esbuild-plugin-glsl": "^1.2.2",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "sst": "^2.40.5",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.4",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
